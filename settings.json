{
    "code-runner.clearPreviousOutput": true,
    "code-runner.executorMap": {
        "bat": "cmd /c",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "javascript": "node",
        "octave": "octave",
        "python": "python -u",
        "typescript": "ts-node"
    },
    "code-runner.runInTerminal": true,
    "debug.toolBarLocation": "commandCenter",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorStyle": "block",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.guides.bracketPairs": "active",
    "editor.guides.bracketPairsHorizontal": false,
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.mouseWheelZoom": true,
    "editor.rulers": [
        80,
        120
    ],
    "editor.stickyScroll.enabled": true,
    "editor.suggestSelection": "first",
    "editor.tabSize": 2,
    "eslint.run": "onSave",
    "eslint.runtime": "node",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "explorer.confirmDelete": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, .eslintrc.json, .prettierrc.json, .gitignore",
        "Dockerfile": ".dockerignore",
        ".env": ".env.*"
    },
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.env*": "properties"
    },
    "files.autoSave": "off",
    "files.eol": "\n",
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/node_modules": true
    },
    "git-graph.date.format": "ISO Date & Time",
    "git-graph.showStatusBarItem": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.dateFormat": "YYYY-MM-DD",
    "gitlens.currentLine.format": "${author, }${date}${' via 'pullRequest}${ â€¢ message|50?}",
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.statusBar.enabled": false,
    "gitlens.views.branches.branches.layout": "list",
    "javascript.preferGoToSourceDefinition": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "jest.autoRun": "off",
    "liveServer.settings.donotVerifyTags": true,
    "notebook.output.scrolling": true,
    "markdown.preview.typographer": true,
    "material-icon-theme.activeIconPack": "react",
    "material-icon-theme.folders.theme": "specific",
    "material-icon-theme.hidesExplorerArrows": true,
    "prettier.arrowParens": "always",
    "prettier.bracketSpacing": true,
    "prettier.endOfLine": "lf",
    "prettier.jsxSingleQuote": true,
    "prettier.printWidth": 80,
    "prettier.quoteProps": "as-needed",
    "prettier.requireConfig": true,
    "prettier.semi": true,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 2,
    "prettier.trailingComma": "es5",
    "prettier.useTabs": false,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.gpuAcceleration": "on",
    "todo-tree.general.showActivityBarBadge": true,
    "typescript.preferGoToSourceDefinition": true,
    "versionlens.caching.duration": 30,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.density.editorTabHeight": "compact",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
    "window.titleSeparator": " | ",
    "workbench.colorTheme": "One Dark Pro",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none"
}